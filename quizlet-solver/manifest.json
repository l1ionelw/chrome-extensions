{
  "manifest_version": 3,
  "name": "Quizlet solver",
  "version": "1.0",
  "description": "solves quizlet problems",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://quizlet.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}